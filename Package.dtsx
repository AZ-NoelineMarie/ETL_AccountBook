        // Variables to fix account 707001 and 707000
       float ecartHT0 = (float)Row.ecartHT0;
       float ecartHT1 = (float)Row.ecartHT1;

        // Variables to fix account 758000 and 658000
        float ecart758 = (float)Row.ecart758;
        float ecart658 = (float)Row.ecart658;
        double ecart445715_758 = 0;
        double ecart707001_758 = 0;
        double ecart445715_658 = 0;
        double ecart707001_658 = 0;

        if (ecart758 != 0)
        {
            ecart707001_758 = (float)Math.Round(ecart758 / 1.055,2);
            ecart445715_758 = ecart758 - ecart707001_758;
       
        }
        if (ecart658 != 0)
        {
            ecart707001_658 = -(float)Math.Round((ecart658 / 1.055), 2);
            ecart445715_658 = - (ecart658 - ecart707001_658) ;
           
