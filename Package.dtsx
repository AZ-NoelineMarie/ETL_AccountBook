        if (strFormatX3.Contains(";445715;"))
        {
            fltPrice = fltPrice + ecartHT1 + (float)ecart445715_758 + (float)ecart445715_658;
            strFormatX3 = strFormatX3.Replace(strPrice, fltPrice.ToString().Replace(",", ".**").Replace("-", ""));
        }



        if (strFormatX3.Contains(";707000;") )
        {
            fltPrice = fltPrice - ecartHT0  ;
            strFormatX3 = strFormatX3.Replace(strPrice, fltPrice.ToString().Replace(",", ".").Replace("-", ""));
        }

        if (strFormatX3.Contains(";445713;") )
        {
            fltPrice = fltPrice + ecartHT0;
            strFormatX3 = strFormatX3.Replace(strPrice, fltPrice.ToString().Replace(",",".").Replace("-", ""));
        }
        

       Sortie0Buffer.AddRow();
       Sortie0Buffer.finalRow = strFormatX3.Replace(",", ".");

    }

