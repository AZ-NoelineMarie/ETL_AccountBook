        // TVAs
        if (strFormatX3.Contains(";707001;"))
        {
            fltPrice = fltPrice - ecartHT1 + (float)ecart707001_758 + (float)ecart707001_658;   
            strFormatX3 = strFormatX3.Replace(strPrice, fltPrice.ToString().Replace(",", ".**").Replace("-", ""));
