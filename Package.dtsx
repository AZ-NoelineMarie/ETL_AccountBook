using System.Threading.Tasks;

namespace SC_6ded6ef434a746b3ad17d597d433a4f8
{
    class LigneFormat
    {
        // Links columns by variables
        string boutique;
        string libelle;
        string nbCpt;
        float fltmontant;
        string date;
        int increm;
        DateTime dateDate;

        // Variables for builder
        string numOp;
        string typeOp;
        string SensOp;
        string montant;
   
        public LigneFormat(string boutique,DateTime dateDate, string nbCpt, string libelle, float fltmontant, int increm)
        {
            this.boutique = boutique;
            this.libelle = libelle;
            this.nbCpt = nbCpt;
            this.fltmontant = fltmontant;
            //this.date = date;
            this.increm = increm ;
            this.dateDate = dateDate;

            montant = fltmontant.ToString().Replace(",", ".");
        }

        override
        public string ToString()
        {
            string str = "";

            libelle += " " + dateDate.Day+ dateDate.Month + dateDate.Year;


            if (increm <= 0){
                str += "G;FAC" + boutique + dateDate.Year + dateDate.Month+ dateDate.Day +"incremFutur" + ";" + boutique + ";VTE;" + dateDate.Year + dateDate.Month + dateDate.Day + ";;EUR\n";
                increm += 1; 
           
            }
            if (nbCpt.Contains("707"))
            {
                SensOp = "-1";
                str = "D;" + increm + ";" + 1 + ";" + boutique + ";;" + nbCpt + ";;" + libelle + ";" + SensOp + ";" + montant + ";EUR\n";
                str += "D;" + increm + ";" + 2 + ";" + boutique + ";;" + nbCpt + ";;" + libelle + ";" + SensOp + ";" + montant + ";EUR\n";
                str += "A;" + 1 + ";AX1;1502;AX2;;AX3;" + boutique + ";;;;;;;;;;;;;0;"+montant ;
            }
            else
            {
                if (nbCpt.Contains("531000") || nbCpt.Contains("532000") || nbCpt.Contains("533000") || nbCpt.Contains("580000"))
                {
                    SensOp = "1";
                    if (nbCpt.Contains("531000") || nbCpt.Contains("532000") || nbCpt.Contains("533000"))
                    {
                        numOp = "411000";
                        typeOp = nbCpt.Contains("531000") ? boutique + "ESP" : nbCpt.Contains("532000") ? boutique + "CHQ" : nbCpt.Contains("533000") ? boutique + "CB" : "!UNK!";
                        str += "D;" + increm + ";" + 1 + ";" + boutique + ";C1;" + numOp + ";" + typeOp + ";" + libelle + ";" + SensOp + ";" + montant + ";EUR";

                    }
                    else { 
                        numOp = "467500";
                        str += "D;" + increm + ";" + 1 + ";" + boutique + ";;" + numOp + ";;" + libelle + ";" + SensOp + ";" + montant + ";EUR";
                    }    
                }
                else
                {
                    str += "D;" + increm + ";" + 1 + ";" + boutique + ";;" + nbCpt + ";;" + libelle + ";" + "-1" + ";" + montant + ";EUR";

                }
            }
            
            return str;
        }
    }
}
