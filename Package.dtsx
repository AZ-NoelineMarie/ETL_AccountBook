                              dataType="System.String"><![CDATA[using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SC_6ded6ef434a746b3ad17d597d433a4f8
{
    
    class LigneFormat
    {
        // Links columns by variables
        string shop;
        string label;
        string idAccount;
        int totalOperation;

        // Variables for builder
        string numOp;
        string typeOp;
        int SensOp = 2;
        float fltPrice;
        string strPrice;
        float priceGiftCard;
        float price703;

        /// <summary>
        /// Constructor
        /// 
        /// </summary>
        /// <param name="shop"></param>
        /// <param name="dtDate"></param>
        /// <param name="idAccount"></param>
        /// <param name="label"></param>
        /// <param name="fltPrice"></param>
        /// <param name="totalOperation"></param>
        /// <param name="priceGiftCard"></param>
        public LigneFormat(string shop, DateTime dtDate, string idAccount, string label, float fltPrice, int totalOperation, float priceGiftCard, float price703)
        {
            this.shop = shop;
            this.label = label + " " + dtDate.ToString("dd") + dtDate.ToString("MM") + dtDate.Year;
            this.idAccount = idAccount;
            this.totalOperation = totalOperation;
            this.fltPrice = fltPrice;
            this.priceGiftCard = priceGiftCard;
            this.price703 = price703;

            strPrice = fltPrice.ToString().Replace("-","");

            // Set sens operation: IF Price is negatif  
            if (fltPrice < 0)
            {            
                if (idAccount.Contains("707") || idAccount.Contains("445713") || idAccount.Contains("445715") || idAccount.Contains("445716")) 
                { SensOp = 1;}
                else{SensOp = 1;}
            }
            else {
                if (idAccount.Contains("707") || idAccount.Contains("445713") || idAccount.Contains("445715") || idAccount.Contains("445716"))  
                { SensOp = -1;}
                else{ SensOp = -1;}
            }
        }

        /// <summary>
        /// Getter for fltPrice
        /// 
        /// 
        /// </summary>
        /// <returns>create inputs float.toString</returns>
        public float getfltPrice()
        {       
                return fltPrice;
        }

        /// <summary>
        /// ToString override method 
        /// 
        /// 
        /// </summary>
        /// <returns>create inputs string</returns>
        override
        public string ToString()
        {
            string str = "";

            
            if (idAccount.Contains("707") && !idAccount.Contains("707003"))
            {
                str += "D;" + totalOperation + ";" + 1 + ";" + shop + ";;" + idAccount + ";;" + label + ";" + SensOp + ";" + strPrice + ";EUR\r\n";
                str += "D;" + totalOperation + ";" + 2 + ";" + shop + ";;" + idAccount + ";;" + label + ";" + SensOp + ";" + strPrice + ";EUR\r\n";
                str += "A;" + 1 + ";AX1;1502;AX2;;AX3;" + shop + ";;;;;;;;;;;;;0;" + strPrice;
            }  

            else if (idAccount.Contains("445713") || idAccount.Contains("445715") || idAccount.Contains("445716"))
            {
                str += "D;" + totalOperation + ";" + 1 + ";" + shop + ";;" + idAccount + ";;" + label + ";" + SensOp +";" + strPrice + ";EUR"; 
            }

            else if (idAccount.Contains("531000") || idAccount.Contains("532000") || idAccount.Contains("533000"))
            { 
                  numOp = "411000";
                  typeOp = idAccount.Contains("531000") ? shop + "ESP" : idAccount.Contains("532000") ? shop + "CHQ" : idAccount.Contains("533000") ? shop + "CB" : "!UNK!";    
                  str += "D;" + totalOperation + ";" + 1 + ";" + shop + ";C1;" + numOp + ";" + typeOp + ";" + label + ";" + SensOp + ";" + strPrice + ";EUR";
            }

            else if (idAccount.Contains("580000"))         
            {
                    str += "D;" + totalOperation + ";" + 1 + ";" + shop + ";;" + "467500" + ";;" + label + ";" + 1 + ";" + strPrice + ";EUR";
                
                    if (fltPrice != 0 && priceGiftCard > 0)
                    {
                        str += "\r\nD;" + (totalOperation + 1) + ";" + 1 + ";" + shop + ";;" + "467500" + ";;" + label + ";-1;" + price703 + ";EUR";
                    }                     
            }
            else if (idAccount.Contains("707003") && priceGiftCard <= 0)
            {
                str = "D;" + totalOperation + ";" + 1 + ";" + shop + ";;" + "467500" + ";;" + label + ";-1;" + price703 + ";EUR"; ;

            }
            else if (idAccount.Contains("707003") && priceGiftCard > 0)
            {
                return "";
            }
            else
            {
                // Can't append : sensOp set by 2
                str = "D;" + totalOperation + ";" + 1 + ";" + shop + ";;" + idAccount + ";;" + label + ";" + 2 + ";" + strPrice + ";EUR\n\r";
  
            }

            return str;
        }
    }
}
