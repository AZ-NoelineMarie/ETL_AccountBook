    public class ComptaFormator
    {
        private char charTypeLigne;
        private string strIncrementation;
        private string strShop;
        private string strVTE;
        private string strNbTransaction;
        private string strTypeTransaction;
        private string strLibelle;
        private int nbSensTransaction;
        private float flPrice;

        public ComptaFormator(char charTypeLigne, string strIncrementation, string strShop, string strVTE,string strNbTransaction,string strTypeTransaction,string strLibelle,int nbSensTransaction,float flPrice){


            this.charTypeLigne = charTypeLigne;
            this.strIncrementation = strIncrementation + ";";
            this.strShop = strShop +";";
            this.strVTE = strVTE + ";";
            this.strNbTransaction = strNbTransaction + ";";
            this.strTypeTransaction = strTypeTransaction + ";";
            this.strLibelle = strLibelle + ";";
            this.nbSensTransaction = nbSensTransaction;
            this.flPrice = flPrice;

            ligneOutput();
    }
        /// <summary>
        /// Methode qui créé la chaine formatée contenant les informations à renvoyer
        /// </summary>
        /// <returns>Une ligne formatée</returns>
     private string ligneOutput()
    {
            string strLigne = "";
            
            switch (charTypeLigne) {
                case 'G': // HEADER
                    strLigne += strLigne + ";FAC;FACAZ--------;" + strShop + strVTE + strNbTransaction + ";" + ";EUR;;;\n";
                    break;
                case 'D': //
                    if (strNbTransaction.Substring(0,3).Equals("707"))
                    {
                        strLigne += strLigne + ";FAC;FACAZ--------;" + strShop + strVTE + strNbTransaction + ";" + ";EUR;;;\n";
                    }
                    break;
                case 'A': // IF analyse
                    break;
            }
        return strLigne;
    }



}
